{
  "name": "matter-platform-web",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "browserslist": {
    "production": [
      "chrome >= 90",
      "firefox >= 88",
      "safari >= 14",
      "edge >= 90"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:analyze": "vite build --mode analyze",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "lint": "eslint src --ext ts,tsx --cache",
    "lint:fix": "eslint src --ext ts,tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,scss}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,css,scss}\"",
    "typecheck": "tsc --noEmit",
    "validate": "npm-run-all --parallel typecheck lint test:ci"
  },
  "dependencies": {
    "@auth0/auth0-react": "^2.0.0",
    "@auth0/auth0-spa-js": "^2.1.0",
    "@axe-core/react": "4.x",
    "@monitoring/core": "^1.0.0",
    "@monitoring/performance": "1.x",
    "@mui/icons-material": "^5.13.0",
    "@mui/material": "^5.11.0",
    "@mui/x-data-grid": "^6.0.0",
    "@reduxjs/toolkit": "^1.9.5",
    "@types/geojson": "^7946.0.10",
    "axios": "1.x",
    "classnames": "2.3.2",
    "d3": "7.8.5",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.0",
    "jwt-decode": "^3.1.2",
    "localforage": "1.10.0",
    "lodash": "4.x",
    "loglevel": "^1.8.1",
    "mapbox-gl": "2.x",
    "newrelic-browser": "^1.0.0",
    "normalize.css": "8.0.1",
    "opossum": "6.x",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.0",
    "react-hook-form": "^7.45.0",
    "react-redux": "^8.1.0",
    "react-router-dom": "^6.0.0",
    "react-virtual": "2.10.4",
    "reconnecting-websocket": "^4.4.0",
    "resize-observer-polyfill": "1.5.1",
    "retry": "^0.13.1",
    "sanitize-html": "^2.7.0",
    "styled-components": "5.3.10",
    "use-debounce": "^9.0.0"
  },
  "devDependencies": {
    "@jest/globals": "29.x",
    "@testing-library/dom": "^9.3.0",
    "@testing-library/jest-dom": "^5.16.0",
    "@testing-library/react": "13.x",
    "@testing-library/user-event": "^14.0.0",
    "@types/jest": "^29.0.0",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@vitejs/plugin-react": "^4.0.0",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.6.0",
    "jest-axe": "7.x",
    "prettier": "^2.8.0",
    "terser": "^5.4.0",
    "ts-jest": "^29.0.0",
    "typescript": "^4.9.5",
    "vite": "^4.0.0",
    "vite-plugin-checker": "^0.6.0",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "0.34.x"
  }
}