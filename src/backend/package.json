{
  "name": "matter-backend-services",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">=18.0.0"
  },
  "workspaces": [
    "auth-service",
    "visualization-service",
    "api-gateway"
  ],
  "scripts": {
    "build": "concurrently \"npm run build --workspace=auth-service\" \"npm run build --workspace=visualization-service\" \"npm run build --workspace=api-gateway\"",
    "start": "concurrently \"npm run start --workspace=auth-service\" \"npm run start --workspace=visualization-service\" \"npm run start --workspace=api-gateway\"",
    "dev": "concurrently \"npm run dev --workspace=auth-service\" \"npm run dev --workspace=visualization-service\" \"npm run dev --workspace=api-gateway\"",
    "test": "jest --coverage",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"**/*.ts\"",
    "clean": "rimraf */dist",
    "prepare": "husky install"
  },
  "dependencies": {
    "@auth0/auth0-spa-js": "^2.0.0",
    "@opentelemetry/api": "1.4.0",
    "@pdfkit/pdfkit": "^3.0.0",
    "bcrypt": "^5.1.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "d3": "^7.8.5",
    "date-fns": "^2.30.0",
    "dotenv": "16.0.3",
    "express": "^4.18.2",
    "express-cache-middleware": "^1.0.0",
    "express-jwt": "^8.4.1",
    "express-rate-limit": "^6.7.0",
    "helmet": "^6.0.1",
    "http-errors": "^2.0.0",
    "http-proxy-middleware": "2.0.6",
    "http-status-codes": "^2.2.0",
    "ioredis": "^5.3.0",
    "jsdom": "^22.1.0",
    "jsonwebtoken": "9.0.2",
    "morgan": "^1.10.0",
    "node-cache": "^5.1.2",
    "pg": "8.11.3",
    "prom-client": "^14.2.0",
    "rate-limit-redis": "3.0.0",
    "rate-limiter-flexible": "^2.4.1",
    "redis": "4.6.10",
    "sanitize-html": "^2.11.0",
    "sharp": "^0.32.5",
    "svg-to-pdfkit": "^0.1.8",
    "winston": "3.11.0",
    "winston-daily-rotate-file": "4.7.1",
    "winston-elasticsearch": "0.17.1"
  },
  "devDependencies": {
    "@commitlint/cli": "17.7.2",
    "@jest/globals": "^29.0.0",
    "@types/d3": "^7.4.0",
    "@types/express": "4.17.17",
    "@types/jest": "^29.5.0",
    "@types/jsdom": "^21.1.1",
    "@types/jsonwebtoken": "9.0.2",
    "@types/node": "18.15.0",
    "cheerio": "^1.0.0-rc.12",
    "concurrently": "8.0.1",
    "eslint": "8.51.0",
    "husky": "8.0.3",
    "ioredis-mock": "^8.0.0",
    "jest": "29.7.0",
    "nock": "13.3.0",
    "prettier": "3.0.3",
    "rimraf": "4.4.0",
    "snyk": "1.1240.0",
    "supertest": "^6.3.3",
    "ts-jest": "29.1.1",
    "typescript": "4.9.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/matter/matter-platform.git"
  },
  "license": "UNLICENSED"
}